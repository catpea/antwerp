{
  "name": "antwerp",
  "version": "1.1.7",
  "description": "Antwerp Static Site Generator",
  "type": "module",
  "main": "antwerp.js",
  "scripts": {
    "save": "git add .; git commit -m 'New Release'; git push; npm version patch; npm publish; git push;",
    "store": "git add .; git commit -m 'New Release'; npm version patch; git push;",
    "watch": "nodemon --watch $(antwerp register catpea) --watch . -e ejs,css,js,json antwerp build catpea",
    "server": "http-server -o -c-1 $(antwerp read catpea configuration.dest)"
  },
  "bin": {
    "antwerp": "./antwerp.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/catpea/antwerp.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/catpea/antwerp/issues"
  },
  "homepage": "https://github.com/catpea/antwerp#readme",
  "dependencies": {
    "ajv": "^8.11.0",
    "calculate-percent": "^2.1.0",
    "chalk": "^5.0.1",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^9.4.0",
    "conf": "^10.2.0",
    "copy-newer": "^2.1.2",
    "ejs": "^3.1.8",
    "ejs-lint": "^1.2.2",
    "front-matter": "^4.0.2",
    "fs-extra": "^10.1.0",
    "fs-extra-promise-es6": "^0.1.1",
    "globby": "^13.1.2",
    "gulp-each": "^0.5.0",
    "html-to-text": "^8.2.1",
    "invariant": "^2.2.4",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "marked": "^4.1.0",
    "moment": "^2.29.4",
    "ms": "^2.1.3",
    "node-fetch": "^3.2.10",
    "pagination": "^0.4.6",
    "pretty": "^2.0.0",
    "progress": "^2.0.3",
    "remark": "^14.0.2",
    "sharp": "^0.31.0",
    "syntax-error": "^1.4.0",
    "turndown": "^7.1.1",
    "unist-util-visit": "^4.1.1",
    "unist-util-visit-parents": "^5.1.1",
    "uuid": "^8.3.2"
  }
}
